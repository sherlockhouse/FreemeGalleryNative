<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.freeme.about">

    <application
        android:allowBackup="true"
        android:supportsRtl="true"
        >
        <activity android:name="com.freeme.about.AboutAcitivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.freeme.intent.action.APP_ABOUT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.freeme.about.AboutDetailActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.freeme.intent.action.APP_CHANNEL_INFO" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
